{
  "manifest_version": 3,
  "name": "Void",
  "version": "1.0",
  "description": "A minimal note-taking extension that replaces your new tab page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
